
@using RMS.Web.Areas.App.Models.Common.Modals
@using RMS.Web.Areas.App.Models.RegistrationJsonDatas
@model RegistrationJsonDataViewModel

@await Html.PartialAsync("~/Areas/App/Views/Common/Modals/_ModalHeader.cshtml", new ModalHeaderViewModel(L("Details")))

<div class="modal-body">
    <div id="RegistrationJsonDataInformationsTab">
        
        <div class="row">
            <div class="col-6 mb-3">
                <h6>@L("Data") :</h6>
                @Model.RegistrationJsonData.Data
            </div>
            <div class="col-6 mb-3">
                <h6>@L("DateCreated") :</h6>
                @Model.RegistrationJsonData.DateCreated
            </div>

            <div class="col-6 mb-3">
                <h6>(@L("Registration")) @L("FirstName") :</h6>
                @Model.RegistrationFirstName
            </div>

        </div>

    </div>
</div>

@await Html.PartialAsync("~/Areas/App/Views/Common/Modals/_ModalFooterWithClose.cshtml")

