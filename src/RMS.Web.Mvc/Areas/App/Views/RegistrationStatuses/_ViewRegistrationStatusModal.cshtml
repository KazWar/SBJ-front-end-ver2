
@using RMS.Web.Areas.App.Models.Common.Modals
@using RMS.Web.Areas.App.Models.RegistrationStatuses
@model RegistrationStatusViewModel

@await Html.PartialAsync("~/Areas/App/Views/Common/Modals/_ModalHeader.cshtml", new ModalHeaderViewModel(L("Details")))

<div class="modal-body">
    <div id="RegistrationStatusInformationsTab">
        
        <div class="row">
            <div class="col-6 mb-3">
                <h6>@L("StatusCode") :</h6>
                @Model.RegistrationStatus.StatusCode
            </div>
            <div class="col-6 mb-3">
                <h6>@L("Description") :</h6>
                @Model.RegistrationStatus.Description
            </div>
            <div class="col-6 mb-3">
                <h6>@L("IsActive") :</h6>
                @Model.RegistrationStatus.IsActive
            </div>


        </div>

    </div>
</div>

@await Html.PartialAsync("~/Areas/App/Views/Common/Modals/_ModalFooterWithClose.cshtml")

